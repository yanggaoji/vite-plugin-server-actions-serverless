{
  "name": "vite-plugin-server-actions",
  "version": "0.1.1",
  "description": "Automatically proxy imported backend functions to a server endpoint in Vite",
  "keywords": [
    "vite-plugin",
    "server",
    "actions"
  ],
  "homepage": "https://github.com/HelgeSverre/vite-plugin-server-actions",
  "bugs": {
    "url": "https://github.com/HelgeSverre/vite-plugin-server-actions/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/HelgeSverre/vite-plugin-server-actions"
  },
  "license": "MIT",
  "author": {
    "name": "Helge Sverre",
    "email": "helge.sverre@gmail.com",
    "url": "https://helgesver.re"
  },
  "type": "module",
  "main": "src/index.js",
  "types": "index.d.ts",
  "scripts": {
    "example:build": "npm run build --prefix examples/todo-app",
    "example:dev": "npm run dev --prefix examples/todo-app",
    "format": "npx prettier --write src/ README.md",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "sort": "npx sort-package-json",
    "test": "vitest",
    "test:run": "vitest run",
    "typecheck": "tsc --noEmit",
    "check": "npm run test:run && npm run lint && npm run typecheck",
    "prepare": "husky"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/express": "^5.0.3",
    "@types/node": "^24.0.3",
    "eslint": "^9.29.0",
    "express": "^4.21.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "minimatch": "^10.0.3",
    "prettier": "^3.3.3",
    "typescript": "^5.8.3",
    "vitest": "^0.34.0"
  },
  "peerDependencies": {
    "vite": "^2.0.0 || ^3.0.0 || ^4.0.0 || ^5.0.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "vitest related --run"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "readme": "README.md"
}
